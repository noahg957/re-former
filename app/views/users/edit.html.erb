<%= form_for(@user) do |f|%>
  <%= f.label :username, "User"%> 
  <%= f.text_field :username %><br><br>
  <%= f.label :email%>
  <%= f.text_field :email, placeholder: "email@email.com" %><br><br>
  <%= f.label :password%>
  <%= f.password_field :password %><br><br>
  <%= f.submit "Submit"%>
<% end%>
